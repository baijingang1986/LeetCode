import java.util.*;
public class Solution {
    public boolean wordBreak(String s, Set<String> dict) {
        int n = s.length();
        boolean dp[] = new boolean[n+1];
        dp[0]= true;
        for(int i = 1; i<=n;i++){
            for(int j = 0;j<i;j++){
                if(dp[j] && dict.contains(s.substring(j,i))){
                    dp[i]= true;
                    break;
                }
            }
        }
        return dp[n];
    }
}
